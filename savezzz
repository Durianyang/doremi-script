  "replace :ZZZZ_ITEMS with :items  where the symbol :ZZZ_ITEMS is the first item in the list "
  (let
    [item-fix (fn[x]
  (cond (and (vector? x)
             (keyword? (first x)) 
             (re-find #"_ITEMS$" (str (first x))))
        (assoc x 0 :items)
        true
        x))
    ] 
      (postwalk item-fix parse-tree)
