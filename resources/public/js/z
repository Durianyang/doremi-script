  var imgLoad = function (x) {
    
    if (false) {
      console.log("imgLoad, x is", x);
    }
    //$('#staff_notation').removeClass("hidden");
  }

  var imgError = function (x) {
    if (debug) {
      console.log("imgError", " x is ", x);
    }
    $('#staff_notation')
      .addClass("hidden");
    setTimeout(reloadLater, 5000);
  }
